{
  "routes": [
    { "src": "/outfits/new(\\?|$)", "dest": "/index.html" },
    { "src": "/outfits/(?<id>[^/]+)", "dest": "/api/outfitPageSSR.js?id=$id" },
    {
      "handle": "filesystem"
    },
    {
      "src": "/static/js/[^.]*\\.[^.]*\\.chunk.js",
      "status": 404
    }
  ],
  "functions": {
    "api/assetImage.js": {
      "excludeFiles": "node_modules/playwright/**"
    }
  }
}
